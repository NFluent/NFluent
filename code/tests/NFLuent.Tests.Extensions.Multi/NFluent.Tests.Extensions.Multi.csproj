  <ItemGroup>
    <ProjectReference Include="..\..\src\NFluent.Multi\NFluent.Multi.csproj" />
  </ItemGroup>

  <Import Project="..\NFluent.Tests.Extensions\NFluent.Tests.Extensions.projitems" Label="Shared" />

</Project>
