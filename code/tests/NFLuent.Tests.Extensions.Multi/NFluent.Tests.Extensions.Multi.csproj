    <Nullable>disable</Nullable>
    <RootNamespace>NFluent.Tests</RootNamespace>
    <IsPublishable>False</IsPublishable>
    <IsTestProject>true</IsTestProject>
    <NoWin32Manifest>true</NoWin32Manifest>
